<config>
    <global>
        <install>
            <!-- Important: validity of installation date indicates whether an application is installed or not -->
            <date><![CDATA[d-d-d-d-d]]></date>
        </install>
        <crypt>
            <key><![CDATA[k-k-k-k-k]]></key>
        </crypt>
        <disable_local_modules>false</disable_local_modules>
        <resources>
            <db>
                <table_prefix><![CDATA[]]></table_prefix>
            </db>
            <default_setup>
                <connection>
                    <host><![CDATA[localhost]]></host>
                    <username><![CDATA[root]]></username>
                    <password><![CDATA[]]></password>
                    <dbname><![CDATA[magento_integration_tests]]></dbname>
                    <initStatements><![CDATA[SET NAMES utf8]]></initStatements>
                    <model><![CDATA[mysql4]]></model>
                    <type><![CDATA[pdo_mysql]]></type>
                    <pdoType><![CDATA[]]></pdoType>
                    <active>1</active>
                </connection>
            </default_setup>
        </resources>
        <session_save><![CDATA[files]]></session_save>
        <resource>
            <connection>
                <types>
                    <pdo_mysql>
                        <adapter><![CDATA[Magento_Test_Db_Adapter_Mysql]]></adapter>
                    </pdo_mysql>
                </types>
            </connection>
        </resource>
        <areas>
            <adminhtml>
                <frontName><![CDATA[backend]]></frontName>
            </adminhtml>
        </areas>
    </global>
</config>
